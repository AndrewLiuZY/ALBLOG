
@{
    ViewData["Title"] = "ChangeAbout";
    Layout = "_SettingsLayout";
}
<div>
    <h4>About</h4>
    <div id="divAbout"></div>
    <hr />
</div>
<div>
    <div>
        <span class="form-group">
            <label>ChangeAbout</label>
            <button class="btn btn-link float-right" style="text-align:right" id="btnSaveAbout">Save</button>
            <button class="btn btn-link float-right" style="text-align:right" id="btnRecallAbout">Recall</button>
            <button class="btn btn-link float-right" style="text-align:right" id="btnPreviewAbout">Preview</button>
        </span>
    </div>
    <div class="margin-top-10" id="editor">
        @Html.Raw(ViewData["context"])
    </div>
    <div>
        <div class="margin-top-10" id="previewBox">
        </div>
    </div>
</div>
<script>
    document.getElementById("navAbout").classList.add("active");
</script>

